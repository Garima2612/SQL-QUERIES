-- 1.Select all records from the Guests table.
SELECT * FROM guests;

-- 2.Select the First_Name and Last_Name of all guests 
SELECT First_Name , Last_Name 
FROM guests;

-- 3.Find the guest with Guest_ID 10.
SELECT * 
FROM guests
WHERE Guest_ID = 10 ;

-- 4.Select the City and Country of all guests, but display only distinct combinations.
SELECT DISTINCT City , Country 
FROM guests;

-- 5.Find all guests who live in 'Springfield'. 
SELECT * 
FROM guests 
WHERE City = 'Springfield';

-- 6.Find the number of guests from each city. 
SELECT City , COUNT(*) AS Number_OF_Guests 
FROM guests
GROUP BY City ;

-- 7. Select guests whose First_Name starts with 'A'.
SELECT * 
FROM guests 
WHERE First_Name LIKE 'A%' ;

-- 8.Update the Phone_Number of the guest with Guest_ID 5 to '999-888-7777'.
UPDATE guests 
SET Phone_Number = '999-888-7777' 
WHERE Guest_ID = '5' ;

-- 9.Delete the guest record with Guest_ID 25. 
DELETE FROM guests 
WHERE Guests_ID = 25 ;

-- 10.Find all guests who have 'gmail' in their email address. 
SELECT * 
FROM guests 
WHERE Email LIKE '%gmail%';

-- 11.Find the total number of guests in the Guests table. 
SELECT COUNT(*) AS Total_guests
FROM guests ;

-- 12.Find the guests who live on a street that contains 'Elm' in the address 
SELECT * 
FROM guests 
WHERE Address LIKE '%Elm%' ;

-- 13.Find the average length of the First_Name of all guests. 
SELECT AVG(Length(First_Name)) AS Average_first_name_length
FROM guests ;

-- 14.Retrieve guests whose phone numbers end with '1234'. 
SELECT * 
FROM Guests 
WHERE Phone_Number LIKE '%1234' ;

 /* 15.List all guests in alphabetical order by Last_Name, and for those with the same Last_Name,
 sort by First_Name.
 */
 SELECT * 
 FROM guests 
 ORDER BY Last_Name , First_Name ;
