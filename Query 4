-- Create Guests table
CREATE TABLE Guests (
    Guest_ID INT PRIMARY KEY,
    First_Name VARCHAR(50),
    Last_Name VARCHAR(50),
    Email VARCHAR(100),
    Phone_Number VARCHAR(15),
    Address VARCHAR(100),
    City VARCHAR(50),
    Country VARCHAR(50)
);

